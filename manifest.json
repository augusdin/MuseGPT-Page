{
  "manifest_version": 3,
  "name": "Muse GPT Page",
  "version": "1.0.0.5",
  "default_locale": "en",
  "description": "Muse GPT Page",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "options_page": "src/pages/options/index.html",
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "host_permissions": [
    "https://cdnjs.buymeacoffee.com/*"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+X"
      },
      "description": "show musegpt page panel"
    }
  },
  "action": {},
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/pages/content/index.js"
      ],
      "css": [
        "assets/css/contentStyle16898323908.chunk.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "assets/png/*.png",
        "assets/svg/*.svg",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}